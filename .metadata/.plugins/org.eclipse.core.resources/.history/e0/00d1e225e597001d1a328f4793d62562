package pageLayer;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

import testBase.TestBase;

public class Contattaci extends TestBase
{

	public Contattaci()
	{
		PageFactory.initElements(driver, this);

	}
	@FindBy(xpath="//span[contains(text(),'Contattaci')]")
	private WebElement click_on_contattaci;
	
	public void checcontattaci()
	{
		click_on_contattaci.click();
	}
	
}
